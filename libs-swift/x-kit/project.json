{
  "root": "libs-swift/x-kit",
  "projectType": "library",
  "targets": {
    "build": {
      "inputs": ["swift-sources"],
      "command": "cd libs-swift/x-kit && swift build"
    },
    "test": {
      "inputs": ["swift-tests"],
      "command": "cd libs-swift/x-kit && SWIFT_DETERMINISTIC_HASHING=1 swift test"
    },
    "format": {
      "inputs": ["swift-sources"],
      "command": "cd libs-swift/x-kit && swiftformat ."
    },
    "format:check": {
      "inputs": ["swift-sources"],
      "command": "cd libs-swift/x-kit && swiftformat --lint ."
    }
  }
}
