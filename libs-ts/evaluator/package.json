{
  "name": "@friends-library/evaluator",
  "version": "1.4.7",
  "description": "evaluate asciidoc AST producing various output formats",
  "main": "./src/index.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "old:lint": "fldev lint --rule '@typescript-eslint/explicit-module-boundary-types: off'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@friends-library/locale": "workspace:*",
    "@friends-library/parser": "workspace:*",
    "@friends-library/types": "workspace:*"
  },
  "devDependencies": {
    "@types/prettier": "2.7.2",
    "prettier": "2.8.3",
    "strip-indent": "^3.0.0"
  }
}
