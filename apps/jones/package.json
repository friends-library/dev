{
  "name": "@friends-library/jones",
  "version": "1.0.0",
  "description": "Friends Library online source doc editor w/ github integration",
  "repository": "github:friends-library-dev/joines",
  "author": "Jared Henderson <jared@netrivet.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "graphql:codegen": "flp-gql-codegen",
    "start": "pnpm parcel --port 8888 ./src/index.html",
    "compile": "true",
    "build": "pnpm parcel build ./src/index.html --no-source-maps",
    "prestart": "npm run write:css",
    "prebuild": "npm run write:css",
    "write:css": "node ./src/write-css.js",
    "test": "fldev test",
    "lint": "fldev lint",
    "lint:fix": "fldev lint:fix",
    "ts:check": "fldev ts:check",
    "format": "fldev format"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@friends-library/adoc-lint": "workspace:*",
    "@friends-library/db": "workspace:*",
    "@friends-library/doc-css": "workspace:*",
    "@friends-library/evaluator": "workspace:*",
    "@friends-library/parser": "workspace:*",
    "@friends-library/types": "workspace:*",
    "@octokit/rest": "^16.8.1",
    "brace": "^0.11.1",
    "buffer": "^5.5.0",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.1.5",
    "escape-string-regexp": "^1.0.5",
    "js-base64": "^2.5.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.14",
    "moment": "^2.24.0",
    "re-resizable": "^4.11.0",
    "react": "^17.0.0",
    "react-ace": "^6.3.2",
    "react-dom": "^17.0.0",
    "react-keyboard-event-handler": "^1.4.1",
    "react-redux": "^6.0.0",
    "react-sizeme": "^2.5.2",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.3.3",
    "smalltalk": "^3.2.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@friends-library/dev": "~4.5.2",
    "@netlify/functions": "^1.0.0",
    "@types/classnames": "^2.2.7",
    "@types/escape-string-regexp": "^1.0.0",
    "@types/js-base64": "^2.3.1",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^11.11.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.9",
    "@types/uuid": "^3.4.4",
    "node-fetch": "^3.2.0",
    "parcel": "^2.8.3"
  },
  "alias": {
    "process": "process/browser.js"
  }
