{
  "name": "@friends-library/evans",
  "description": "Friends Library websites (English and Spanish)",
  "author": "Jared Henderson <jared@netrivet.com>",
  "repository": "github:friends-library-dev/evans",
  "main": "./dist/build/query.js",
  "version": "1.0.1",
  "scripts": {
    "develop:en": "GATSBY_LANG=en GATSBY_PORT=9222 GATSBY_ALT_PORT=9333 gatsby develop --port 9222",
    "develop:es": "GATSBY_LANG=es GATSBY_PORT=9333 GATSBY_ALT_PORT=9222 gatsby develop --port 9333",
    "prebuild": "npm run cover-css",
    "predevelop:en:site": "npm run cover-css",
    "predevelop:es:site": "npm run cover-css",
    "build": "gatsby build && cp ./_redirects ./public/ && cp ./_headers ./public/",
    "build:en": "GATSBY_LANG=en npm run build",
    "build:es": "GATSBY_LANG=es npm run build",
    "dpc-cache:clean": "rm -f src/build/.dpc-cache.json",
    "cover-css": "fldev tsnode ./src/build/cover-static-css.ts",
    "analyze": "ANALYZE_BUNDLE_SIZE=true npm run build:en",
    "start": "npm run develop:en",
    "test": "fldev test",
    "compile": "true",
    "lint": "fldev lint --rule 'react/no-unescaped-entities: 1'",
    "lint:fix": "npm run lint -- --fix",
    "ts:check": "fldev ts:check",
    "format": "fldev format",
    "pregraphql:codegen": "mv src/build/schema.{graphql,txt}; true",
    "postgraphql:codegen": "mv src/build/schema.{txt,graphql}",
    "graphql:codegen": "flp-gql-codegen --convert-dates-to-string",
    "npub:precheck": "fldev ci && npm run prepublishOnly",
    "prepublishOnly": "tsc src/build/query.ts --skipLibCheck --target esnext --moduleResolution node --outDir dist/ --declaration"
  },
  "dependencies": {
    "@friends-library/types": "^7.0.0",
    "graphql": "14.7.0",
    "x-syntax": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@friends-library/cover-component": "^3.1.6",
    "@friends-library/db": "^3.1.0",
    "@friends-library/dev": "~4.5.2",
    "@friends-library/dpc-fs": "^4.0.2",
    "@friends-library/env": "^1.4.0",
    "@friends-library/friend-repos": "^1.1.1",
    "@friends-library/locale": "^2.5.0",
    "@friends-library/lulu": "^2.0.1",
    "@friends-library/theme": "^1.2.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@reach/accordion": "^0.11.2",
    "@reach/dialog": "^0.11.2",
    "@types/classnames": "^2.2.10",
    "@types/fs-extra": "^9.0.1",
    "@types/he": "^1.1.1",
    "@types/js-cookie": "^2.2.6",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^17.0.47",
    "@types/react-helmet": "^6.1.0",
    "@types/react-instantsearch-dom": "^6.3.0",
    "@types/react-stripe-elements": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "algoliasearch": "^4.5.1",
    "babel-plugin-typescript-strip-namespaces": "^1.1.1",
    "babel-preset-gatsby": "^0.5.13",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.1.5",
    "eslint": "^7.9.0",
    "filesize": "^6.1.0",
    "fs-extra": "^9.0.1",
    "gatsby": "^2.24.66",
    "gatsby-background-image-preact": "^1.1.2-alpha.1",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-google-fonts-with-attributes": "^1.0.7",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-mdx": "^1.2.44",
    "gatsby-plugin-postcss": "^3.0.1",
    "gatsby-plugin-preact": "^4.0.15",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.13",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-plugin-sitemap": "^2.4.15",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-sharp": "^2.5.16",
    "he": "^1.2.0",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "preact": "^10.5.4",
    "preact-render-to-string": "^5.1.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.7.0",
    "react-stripe-elements": "^6.1.2",
    "react-swipeable": "^5.5.1",
    "react-switch": "^5.0.1",
    "really-small-events": "^1.1.0",
    "tailwindcss": "^1.8.10",
    "use-interval": "^1.3.0",
    "x-chalk": "^1.0.1"
  },
  "overrides": {
    "graphql": "14.7.0"
  }
}
