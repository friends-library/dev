{
  "namedInputs": {
    "swift-sources": ["{projectRoot}/Sources/**/*.swift", "{projectRoot}/Package.swift"],
    "swift-tests": [
      "{projectRoot}/Sources/**/*.swift",
      "{projectRoot}/Tests/**/*.swift",
      "{projectRoot}/Package.swift"
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "typecheck",
          "test",
          "lint",
          "lint:fix",
          "format",
          "format:check",
          "build",
          "compile"
        ]
      }
    }
  },
  "targetDefaults": {},
  "defaultBase": "master"
}
